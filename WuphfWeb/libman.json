{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "bootstrap@5.2.3",
      "destination": "wwwroot/lib/bootstrap/dist/",
      "files": [
        "css/*",
        "js/*"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap-icons@1.10.3",
      "destination": "wwwroot/lib/bootstrap-icons/dist/"
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr/dist/"
    },
    {
      "library": "globalize@1.7.0",
      "destination": "wwwroot/lib/globalize/",
      "files": [ "globalize.js" ]
    },
    {
      "library": "cldrjs@0.5.5",
      "destination": "wwwroot/lib/cldrjs/",
      "files": [ "cldr.js" ]
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation-globalize@0.1.1",
      "destination": "wwwroot/lib/jquery-validation-globalize/",
      "files": [ "jquery.validate.globalize.js" ]
    },
    {
      "library": "sweetalert2@11.7.2",
      "destination": "wwwroot/lib/sweetalert2/dist/"
    },
    {
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcodejs/"
    }
  ]
}